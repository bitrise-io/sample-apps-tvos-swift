project 'NPO Live.xcodeproj'

# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'NPO Live' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    pod "NPOStream", "~> 0.0.6"
    
    # Pods for iOS Sample
    
    target 'NPO LiveTests' do
        inherit! :search_paths
        # Pods for testing
        use_frameworks!
        pod "NPOStream", "~> 0.0.6"
    end
    
    target 'NPO LiveUITests' do
        inherit! :search_paths
        # Pods for testing
        use_frameworks!
        pod "NPOStream", "~> 0.0.6"
    end
    
end

